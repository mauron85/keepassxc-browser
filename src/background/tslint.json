{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "member-ordering": [
            true,
            {
                "order": [
                    "static-field",
                    "instance-field",
                    "constructor",
                    "public-instance-method",
                    "protected-instance-method",
                    "private-instance-method",
                    "public-static-method"
                ]
            }
        ],
        "ordered-imports": [
            true,
            {
                "import-sources-order": "lowercase-first",
                "named-imports-order": "lowercase-first"
            }
        ],
        "arrow-parens": false,
        "quotemark": ["single"],
        "object-literal-sort-keys": false,
        "interface-name": [false, "never-prefix"],
        "no-bitwise": false,
        "trailing-comma": [
            true,
            {
                "multiline": {
                    "objects": "ignore",
                    "arrays": "ignore",
                    "functions": "never",
                    "typeLiterals": "ignore"
                },
                "singleline": "never"
            }
        ]
    },
    "rulesDirectory": []
}